{
  "id": "first",
  "aside": "Options",
  "children": [
    {
      "Organism.Header": {
        "children": [
          {
            "Atom.Heading": {
              "text": "Atoms"
            }
          },
          {
            "Molecule.Navigation": {
              "style": "left",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "navicon",
                    "path": "aside"
                  }
                }
              ]
            }
          },
          {
            "Molecule.Navigation": {
              "style": "right",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "forward",
                    "path": "second/main"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
        "Molecule.Navigation": {
            "children": [
                {
                  "Atom.Button": {
                    "style": "active",
                    "text": "form",
                    "icon": "clipboard",
                    "path": "first/form"
                  }
                },
                {
                  "Atom.Button": {
                    "text": "list",
                    "icon": "navicon",
                    "path": "first/list"
                  }
                },
                {
                  "Atom.Button": {
                    "text": "Map",
                    "icon": "map",
                    "path": "first/map"
                  }
                },
                {
                  "Atom.Button": {
                    "text": "Carousel",
                    "icon": "image",
                    "path": "first/carousel"
                  }
                }
              ]
        }
    },
    {
      "Organism.Section": {
        "id": "form",
        "style": "padding",
        "children": [
          {
            "Molecule.Search": {
              "events": [
                "enter"
              ],
              "children": [
                {
                  "Atom.Input": {
                    "placeholder": "Search..."
                  }
                },
                {
                  "Atom.Button": {
                    "icon": "search"
                  }
                }
              ]
            }
          },
          {
            "Molecule.Form": {
              "events": [
                "keyup",
                "keypress",
                "submit"
              ],
              "children": [
                {
                  "Atom.Label": {
                    "text": "General Info"
                  }
                },
                {
                  "Atom.Input": {
                    "type": "text",
                    "name": "name",
                    "placeholder": "Type your name...",
                    "label": "Your credentials",
                    "required": true,
                    "events": [
                      "keyup",
                      "keypress",
                      "swipe"
                    ]
                  }
                },
                {
                  "Atom.Select": {
                    "name": "options",
                    "value": 3,
                    "options": [
                      {
                        "label": "One",
                        "value": 1
                      },
                      {
                        "label": "Two",
                        "value": 2
                      },
                      {
                        "label": "Three",
                        "value": 3
                      }
                    ],
                    "events": [
                      "change"
                    ]
                  }
                },
                {
                  "Atom.Input": {
                    "type": "password",
                    "name": "password",
                    "placeholder": "Type your password...",
                    "events": [
                      "keyup"
                    ]
                  }
                },
                {
                  "Atom.Label": {
                    "text": "Other Information"
                  }
                },
                {
                  "Atom.Input": {
                    "type": "email",
                    "name": "mail",
                    "placeholder": "Type your mail..."
                  }
                },
                {
                  "Atom.Input": {
                    "type": "search",
                    "name": "Search...",
                    "style": "big",
                    "placeholder": "Typer your username..."
                  }
                },
                {
                  "Atom.Textarea": {
                    "placeholder": "Text..."
                  }
                },
                {
                  "Atom.Input": {
                    "type": "number",
                    "name": "years",
                    "placeholder": "Years..."
                  }
                },
                {
                  "Atom.Input": {
                    "type": "range",
                    "name": "years",
                    "placeholder": "Years..."
                  }
                },
                {
                  "Atom.Switch": {
                    "name": "onoff",
                    "value": true,
                    "events": [
                      "change"
                    ]
                  }
                },
                {
                  "Atom.Switch": {
                    "name": "onoff2",
                    "value": false,
                    "events": [
                      "touch"
                    ]
                  }
                },
                {
                  "Atom.Button": {
                    "text": "Save",
                    "icon": "checkmark",
                    "style": "accept fluid"
                  }
                }
              ]
            }
          },
          {
            "Atom.Button": {
              "style": "fluid",
              "disabled": true,
              "icon": "search",
              "text": "Find your friends",
              "events": [
                "touch"
              ]
            }
          }
        ]
      }
    },
    {
      "Organism.Section": {
        "id": "list"
      }
    },
    {
      "Organism.GMapFullScreen": {
        "id": "map"
      }
    },
    {
      "Organism.Section": {
        "id": "carousel",
        "children": [
          {
            "Molecule.Carousel": {
              "id": "carousel_manager",
              "children": [
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?1"
                  }
                },
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?2"
                  }
                },
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?3"
                  }
                },
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?4"
                  }
                },
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?5"
                  }
                },
                {
                  "Atom.Figure": {
                    "url": "http://lorempixel.com/400/200/?6"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "Organism.Footer": {
        "children": [
          {
            "Molecule.Navigation": {
              "children": [
                {
                  "Atom.Button": {
                    "style": "active",
                    "icon": "archive",
                    "path": "first/form"
                  }
                },
                {
                  "Atom.Button": {
                    "icon": "clipboard",
                    "path": "first/list"
                  }
                },
                {
                  "Atom.Button": {
                    "icon": "map",
                    "path": "first/map"
                  }
                },
                {
                  "Atom.Button": {
                    "icon": "image",
                    "path": "first/carousel"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
