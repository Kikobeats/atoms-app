{
  "id": "form",
  "aside": "Options",
  "children": [
    {
      "Organism.Header": {
        "children": [
          {
            "Atom.Heading": {
              "text": "Atoms"
            }
          },
          {
            "Molecule.Navigation": {
              "style": "left",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "navicon",
                    "path": "aside"
                  }
                }
              ]
            }
          },
          {
            "Molecule.Navigation": {
              "style": "right",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "forward",
                    "path": "list/contacts"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "Organism.Section": {
        "id": "main",
        "style": "padding",
        "children": [
          {
            "Molecule.Form": {
              "events": [
                "submit",
                "error",
                "change"
              ],
              "children": [
                {
                  "Atom.Label": {
                    "text": "General Info"
                  }
                },
                {
                  "Atom.Input": {
                    "type": "text",
                    "name": "name",
                    "placeholder": "Type your name...",
                    "label": "Your credentials",
                    "required": true,
                    "value"   : "@soyjavi",
                    "events": ["keyup"]
                  }
                },
                {
                  "Atom.Select": {
                    "name": "options",
                    "value": 3,
                    "required": true,
                    "options": [
                      {
                        "label": "One",
                        "value": 1
                      },
                      {
                        "label": "Two",
                        "value": 2
                      },
                      {
                        "label": "Three",
                        "value": 3
                      }
                    ],
                    "events": [ "change" ]
                  }
                },
                {
                  "Atom.Input": {
                    "type": "password",
                    "name": "password",
                    "placeholder": "Type your password...",
                    "events": ["keyup"]
                  }
                },
                {
                  "Atom.Label": {
                    "text": "Other Information"
                  }
                },
                {
                  "Atom.Input": {
                    "type": "email",
                    "name": "mail",
                    "placeholder": "Type your mail..."
                  }
                },
                {
                  "Atom.Input": {
                    "type": "search",
                    "name": "search",
                    "style": "big",
                    "placeholder": "Type a search..."
                  }
                },
                {
                  "Atom.Textarea": {
                    "placeholder": "Text...",
                    "name": "description"
                  }
                },
                {
                  "Atom.Input": {
                    "type": "number",
                    "name": "years",
                    "placeholder": "Years..."
                  }
                },
                {
                  "Atom.Input": {
                    "type": "range",
                    "name": "years",
                    "placeholder": "Years..."
                  }
                },
                {
                  "Atom.Switch": {
                    "name": "onoff",
                    "value": true,
                    "events": [
                      "change"
                    ]
                  }
                },
                {
                  "Atom.Switch": {
                    "name": "onoff2",
                    "value": false,
                    "events": [
                      "touch"
                    ]
                  }
                },
                {
                  "Atom.Button": {
                    "text": "Save",
                    "icon": "checkmark",
                    "style": "accept fluid"
                  }
                }
              ]
            }
          },
          {
            "Atom.Button": {
              "style": "fluid",
              "icon": "search",
              "text": "Find your friends",
              "events": ["touch"]
            }
          },
          {
            "Atom.Button": {
              "style": "fluid",
              "disabled": true,
              "text": "Disabled",
              "events": []
            }
          },
          {
            "Molecule.Navigation": {
              "style": "margin-top",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "checkmark"
                  }
                },
                {
                  "Atom.Button": {
                    "style": "accept",
                    "icon": "checkmark"
                  }
                },
                {
                  "Atom.Button": {
                    "text": "example"
                  }
                }
              ]
            }
          }

        ]
      }
    }
  ]
}
