{
  "id": "first",
  "aside": "Options",
  "children": [
    {
      "Organism.Header": {
        "children": [
          {
            "Atom.Heading": {
              "text": "Contacts"
            }
          },
          {
            "Molecule.Navigation": {
              "style": "left",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "back",
                    "path": "back"
                  }
                }
              ]
            }
          },
          {
            "Molecule.Navigation": {
              "style": "right",
              "children": [
                {
                  "Atom.Button": {
                    "icon": "person-b-add",
                    "callbacks": ["onLoadContacts"]
                  }
                },
                {
                  "Atom.Button": {
                    "icon": "forward",
                    "path": "second/main"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    {
      "Molecule.Navigation": {
        "children": [
          {
            "Molecule.Search": {
              "events": ["change", "enter"]
            }
          }
        ]
      }
    },
    {
      "Organism.Section": {
        "id": "contacts",
        "children": [
          {
            "Molecule.List": {
              "id"          : "list",
              "bind": {
                "entity"    : "Contact",
                "atom"      : "Atom.Li",
                "events"    : ["tap", "swipeLeft"],
                "callbacks" : ["onContactUpdate", "onContactDelete"],
                "auto"      : true
              }
            }
          }
        ]
      }
    }
  ]
}
